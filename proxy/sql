CREATE TABLE IF NOT EXISTS tbl_proxy
                (`id` INTEGER PRIMARY KEY AUTOINCREMENT,
                 `proxy_url` TEXT,
                 `weightage` INT,
                 `weightage_minute` INT,
                 `event_time` TEXT,
                 `proxy_name` TEXT)


